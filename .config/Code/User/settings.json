    "vim.incsearch": true,
    "vim.ignorecase": true,
    "vim.useSystemClipboard":true,
    "vim.hlsearch": true,
    "vim.easymotion": true,
    "vim.visualstar": true,
    "vim.useCtrlKeys": true,

    "vim.enableNeovim": true,
    "vim.neovimPath": "/usr/bin/nvim",

    "vim.leader": "<space>",

    "vim.statusBarColorControl": true,
    "vim.statusBarColors.normal": "#0b173b",
    "vim.statusBarColors.insert": "#0b3b17",
    "vim.statusBarColors.visual": "#240b3b",
    "vim.statusBarColors.visualblock": "#240b3b",
    "vim.statusBarColors.visualline": "#240b3b",
    "vim.statusBarColors.replace": "#3b0b17",
