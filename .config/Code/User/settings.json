{
    "http.proxy": "http://wwwproxy.kanazawa-it.ac.jp:8080",
    "editor.fontSize": 14,
    "terminal.integrated.fontSize": 14,
    "files.eol": "\n",
    "files.autoGuessEncoding": true,
